//Problem 17

int main() {
    double gpa;
    cout << "Enter your GPA (0-4.5): ";
    cin >> gpa;
    int a = gpa * 10;
    switch (a) {
    case 40 ... 45:
        cout << "You got 80% scholarship." << endl;
        break;
    case 35 ... 39:
        cout << "You got 60% scholarship." << endl;
        break;
    case 30 ... 34:
        cout << "You got 50% scholarship." << endl;
        break;
    default:
        cout << "No scholarship." << endl;
    }
    return 0;
}
